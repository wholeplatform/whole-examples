                                <workflows:StringLiteral value="cos(double)"/>
                                <workflows:Expressions initialCapacity="1">
                                    <workflows:Variable value="radians"/>
                                </workflows:Expressions>
                            </workflows:InvokeJavaClassMethod>
                        </queries:Block>
                    </queries:QueryDeclaration>
                    <queries:QueryDeclaration>
