                                    </queries:If>
                                    <queries:If>
                                        <queries:TypeTest value="http://oslo.crossexamples.whole.org/Logo#Move"/>
                                        <queries:PointwiseUpdate>
                                        <queries:Sequence initialCapacity="2">
                                        <queries:VariableRefStep value="x"/>
                                        <queries:VariableRefStep value="y"/>
                                        </queries:Sequence>
