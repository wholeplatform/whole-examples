<?xml version="1.0"?>
<patterns:PatternLanguage xmlns:patterns="whole:org.whole.lang.patterns:PatternsModel">
    <patterns:URI value="whole:org.whole.crossexamples.cg13.patterns.reuse:ModulesPatterns"/>
    <patterns:Namespace value="org.whole.crossexamples.cg13.patterns.reuse"/>
    <patterns:Name value="ModulesPatterns"/>
    <commons:Resolver xmlns:commons="http://lang.whole.org/Commons"/>
    <patterns:Patterns initialCapacity="5">
        <patterns:Pattern>
            <patterns:ResultTypes initialCapacity="1">
                <models:AnyType xmlns:models="http://lang.whole.org/Models"/>
            </patterns:ResultTypes>
            <patterns:Name value="ReuseArtifacts"/>
            <patterns:DerivationPoint>
                <queries:Scope xmlns:queries="http://lang.whole.org/Queries">
                    <queries:Names initialCapacity="1">
                        <queries:Name value="__artifactsModel__"/>
                    </queries:Names>
                    <queries:Path initialCapacity="2">
                        <workflows:LoadArtifacts xmlns:workflows="http://lang.whole.org/Workflows">
                            <workflows:Text value="Include Artifacts"/>
                            <workflows:Variable value="__artifactsModel__"/>
                            <patterns:Slot>
                                <patterns:Name value="kind"/>
                                <patterns:BooleanValue value="true"/>
                                <queries:Choose initialCapacity="2">
                                    <queries:If>
                                        <queries:KindTest value="RESOLVER"/>
                                        <commons:StageUpFragment xmlns:commons="http://lang.whole.org/Commons">
                                        <workflows:ResourceKind value="WORKSPACE"/>
                                        </commons:StageUpFragment>
                                    </queries:If>
                                    <queries:Do>
                                        <queries:SelfStep/>
                                    </queries:Do>
                                </queries:Choose>
                                <commons:Resolver xmlns:commons="http://lang.whole.org/Commons"/>
                                <commons:Resolver xmlns:commons="http://lang.whole.org/Commons"/>
                            </patterns:Slot>
                            <patterns:Slot>
                                <patterns:Name value="uri"/>
                                <patterns:BooleanValue value="false"/>
                                <commons:Resolver xmlns:commons="http://lang.whole.org/Commons"/>
                                <models:EntityType xmlns:models="http://lang.whole.org/Models" value="http://lang.whole.org/Workflows#StringLiteral"/>
                                <commons:Resolver xmlns:commons="http://lang.whole.org/Commons"/>
                            </patterns:Slot>
                            <workflows:ArtifactsTraversalStrategy value="DEEP_DIRECTORY"/>
                            <workflows:ArtifactsSynchronizeStrategy value="ADD_UPDATE"/>
                            <commons:Resolver xmlns:commons="http://lang.whole.org/Commons"/>
                        </workflows:LoadArtifacts>
                        <patterns:Slot>
                            <patterns:Name value="adapter"/>
                            <patterns:BooleanValue value="true"/>
                            <queries:Choose initialCapacity="3">
                                <queries:If>
                                    <queries:KindTest value="RESOLVER"/>
                                    <commons:StageUpFragment xmlns:commons="http://lang.whole.org/Commons">
                                        <queries:SelfStep/>
                                    </commons:StageUpFragment>
                                </queries:If>
                                <queries:If>
                                    <queries:ExtendedSubtypeTest value="http://lang.whole.org/Queries#StepExpression"/>
                                    <queries:SelfStep/>
                                </queries:If>
                                <queries:Do>
                                    <commons:StageUpFragment xmlns:commons="http://lang.whole.org/Commons">
                                        <queries:Sequence initialCapacity="1">
                                        <commons:StageDownFragment>
                                        <queries:SelfStep/>
                                        </commons:StageDownFragment>
                                        </queries:Sequence>
                                    </commons:StageUpFragment>
                                </queries:Do>
                            </queries:Choose>
                            <models:EntityType xmlns:models="http://lang.whole.org/Models" value="http://lang.whole.org/Queries#PathExpression"/>
                            <commons:Resolver xmlns:commons="http://lang.whole.org/Commons"/>
                        </patterns:Slot>
                    </queries:Path>
                </queries:Scope>
                <commons:Resolver xmlns:commons="http://lang.whole.org/Commons"/>
            </patterns:DerivationPoint>
        </patterns:Pattern>
        <patterns:Pattern>
            <patterns:ResultTypes initialCapacity="1">
                <models:AnyType xmlns:models="http://lang.whole.org/Models"/>
            </patterns:ResultTypes>
            <patterns:Name value="ReuseResource"/>
            <patterns:DerivationPoint>
                <queries:Path xmlns:queries="http://lang.whole.org/Queries" initialCapacity="2">
                    <workflows:LoadModel xmlns:workflows="http://lang.whole.org/Workflows">
